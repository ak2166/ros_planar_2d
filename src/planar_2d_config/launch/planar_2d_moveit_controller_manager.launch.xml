<launch>
   <arg name="moveit_controller_manager" default="planar_2d_moveit_controller_manager/Planar2DMoveItControllerManager" />
   <param name="moveit_controller_manager" value="$(arg moveit_controller_manager)"/>
   <arg name="controller_manager_name" default="planar_2d_controller_manager" />
   <param name="controller_manager_name" value="$(arg controller_manager_name)" />
   <param name="use_controller_manager" value="$(arg use_controller_manager)" />
   <rosparam file="$(find planar_2d_config)/controllers.yaml"/>
</launch>
